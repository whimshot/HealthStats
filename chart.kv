#:kivy 1.10.0

<Chart>
    keep_ratio: False
    allow_stretch: True

<WeightChart>
    feeds: ['weight', 'bmi']
    source: 'WeightChart.png'

<BPChart>
    feeds: ['systolic', 'diastolic', 'pulse']
    source: 'BPChart.png'

<SmallCharts>
    feeds: ['weight', 'bmi', 'systolic', 'diastolic', 'pulse']
    source: 'SmallCharts.png'

<ChartDemo>
    weightchart: weight_chart
    bpchart: bp_chart
    smallcharts: small_charts
    WeightChart:
        id: weight_chart
    BPChart:
        id: bp_chart
    SmallCharts:
        id: small_charts
